<Bindings>
  <Binding name="RHLIB_ON" description="Auto rotation ON" header="RHLIB">
    AutoRotationOn();
  </Binding>
  <Binding name="RHLIB_OFF" description="Auto rotation OFF" header="RHLIB">
    AutoRotationOff();
  </Binding>
  <Binding name="RHLIB_DEBUG" description="Toggle Debug Mode" header="RHLIB">
    DebugToggle();
  </Binding>
  <Binding name="RHLIB_RELOAD" description="Reload UI" header="RHLIB">
    ReloadUI();
  </Binding>
  <Binding name="RHLIB_TRASH" description="Trash Toggle" header="RHLIB">
    TrashToggle();
  </Binding>
</Bindings>
